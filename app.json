{
  "expo": {
    "name": "uportal",
    "slug": "uportal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/UportalSplash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000"
    },
    "userInterfaceStyle": "automatic",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/UportalLogo.png",
        "backgroundColor": "#000"
      },
      "package": "com.poa.uportal"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
